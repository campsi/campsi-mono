    "eslint-config-axeptio": "^1.0.0",
    "eslint-plugin-n": "^15.2.0",
    "express-async-errors": "^3.1.1",
    "express-json-validator-middleware": "^2.2.1",
    "express-session": "^1.16.1",
    "http-errors": "^2.0.0",
    "ioredis": "^4.28.0",
    "jsdom": "^22.1.0",
    "json-schema-ref-parser": "^5.0.3",
    "just-diff": "^5.0.1",
    "mkdirp": "^0.5.5",
    "mongo-uri-builder": "^1.0.3",
    "mqtt-emitter": "^1.2.4",
    "passport-github2": "^0.1.10",
    "pino-datadog-transport": "^1.2.2",
    "pino-http": "^7.0.0",
    "request": "^2.88.2",
    "string-format": "^2.0.0",
    "stripe": "^8.194.0",
    "unique-slug": "^2.0.0",
    "yargs": "^16.1.0"
  },
  "devDependencies": {
