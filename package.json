{
  "name": "campsi",
  "version": "5.15.3",
  "description": "Configurable API for managing and publishing document-oriented content",
  "main": "index.js",
  "scripts": {
    "depcheck": "depcheck --ignores=path,mocha,mocha-lcov-reporter",
    "lint": "eslint --no-error-on-unmatched-pattern lib/** test/**",
    "test-campsi": "docker-compose up -d && mocha \"./test/campsi/*.js\" --exit",
    "test-assets": "docker-compose up -d && mocha \"./test/assets/*.js\" --exit",
    "test-auth": "docker-compose up -d && mocha \"./test/auth/*.js\" --ignore './test/auth/collection-name*.js' --exit",
    "test-auth-collection-name-override": "docker-compose up -d && mocha \"./test/auth/collection-name.js\" --exit",
    "test-auth-collection-name-default": "docker-compose up -d && mocha \"./test/auth/collection-name-default.js\" --exit",
    "test-docs": "docker-compose up -d && mocha  \"./test/docs/*.js\" --exit",
    "test-webhooks": "docker-compose up -d && mocha \"./test/webhooks/*.js\" --exit",
    "test-trace": "docker-compose up -d && mocha \"./test/trace/*.js\" --exit",
    "test-versioned-docs": "docker-compose up -d && mocha  \"./test/versioned-docs/*.js\" --exit",
    "test-stripe-billing": "docker-compose up -d && mocha \"./test/stripe-billing/*.js\" --exit",
    "test-notifications": "docker-compose up -d && mocha \"./test/notifications/*.js\" --exit",
    "test-locks": "docker-compose up -d && mocha  \"./test/docs/lock-document.js\" --exit",
    "test-crud": "docker-compose up -d && mocha  \"./test/docs/crud.js\" --exit",
    "test-audit": "docker-compose up -d && mocha \"./test/audit/*.js\" --exit",
    "test": "docker-compose up -d && mocha -R dot './test/{,!(launchers)/**/}/*.js' --ignore './test/auth/collection-name*.js' --exit && mocha -R dot './test/auth/collection-name.js' --exit",
    "prepare": ""
  },
  "eslintIgnore": [
    "test/data/*",
    "test/data"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/campsi/campsi-mono.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/campsi/campsi-mono/issues"
  },
  "homepage": "https://github.com/campsi/campsi-mono#readme",
  "peerDependencies": {
    "async": "^2.6.3",
    "body-parser": "^1.18.2",
    "debug": "^3.2.6",
    "express": "^4.16.4",
    "mongodb": "^6.3.0"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.398.0",
