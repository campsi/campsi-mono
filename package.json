    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "config": "^3.2.2",
    "coveralls": "^3.0.4",
    "debug": "^3.2.6",
    "depcheck": "^1.2.0",
    "eslint": "^8.13.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-indexof": "^0.1.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "express": "^4.16.4",
    "fake-object-id": "0.0.3",
    "find-process": "^1.4.7",
    "husky": "^8.0.1",
    "mime-types": "^2.1.18",
    "mocha": "^10.0.0",
    "mocha-lcov-reporter": "^1.3.0",
    "path": "^0.12.7",
    "prettier": "^2.7.1",
    "rimraf": "^2.7.1",
    "semantic-release": "^22.0.12",
    "sinon": "^14.0.0",
    "sinon-chai": "^3.7.0"
  },
  "files": [
    "LICENSE",
    "README.md",
    "index.js",
    "lib/",
    "services/"
  ],
  "engines": {
    "node": ">=16.20.1",
    "npm": ">=7"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint; npm run test"
    }
  }
}
